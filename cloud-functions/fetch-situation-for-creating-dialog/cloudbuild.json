{
  "steps": [
    {
      "name": "gcr.io/google.com/cloudsdktool/cloud-sdk",
      "dir": "cloud-functions/fetch-situation-for-creating-dialog",
      "args": [
        "gcloud",
        "functions",
        "deploy",
        "fetchSituationForCreatingDialog",
        "--gen2",
        "--region=us-east1",
        "--source=.",
        "--runtime=nodejs18",
        "--entry-point=fetchSituationForCreatingDialog",
        "--trigger-topic=fetch-situation-for-creating-dialog"
      ],
      "secretEnv": ["DATABASE_URL", "SERVICE_ACCOUNT"]
    }
  ],
  "availableSecrets": {
    "secretManager": [
      {
        "versionName": "projects/625494448696/secrets/DATABASE_URL/versions/2",
        "env": "DATABASE_URL"
      },
      {
        "versionName": "projects/625494448696/secrets/SERVICE_ACCOUNT/versions/1",
        "env": "SERVICE_ACCOUNT"
      }
    ]
  },
  "options": { "logging": "CLOUD_LOGGING_ONLY" }
}
